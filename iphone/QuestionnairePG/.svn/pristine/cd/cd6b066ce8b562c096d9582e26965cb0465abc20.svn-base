@charset "UTF-8";
/*新调查问卷start*/
.b{border: 1px solid red;background-color: #999999;}
a{ text-decoration:none}

input,textarea{  outline: none;  -webkit-appearance: none; -webkit-tap-highlight-color: rgba(0, 0, 0, 0); }
.indent2{text-indent: 2em;}
/*问卷首页信息简介  start*/
#homePage{min-height: 100%;  height: auto!important;  height: 100%;  position: relative;  padding-bottom:20px;background-color: #edebec;}
#homePage .content{max-width: 800px;margin: auto;position: relative;}
#homePage .survey-img{width: 100%;height: auto;position: static;}
#homePage .survey-img img{display: block;width: 100%;  height: 180px;}
#homePage .survey-content{padding: 0 10px 40px 10px;  padding-top: 0px;font-size: 15px;color: #333333;position: static;top: 180px;height: calc(100% - 224px);overflow: auto;}
#homePage .surveyDepict {background-color: #ffffff;}
#homePage .surveyDepict table{  border:1px solid #e6e6e6;}
#homePage .surveyDepict table tbody tr td{border-top: 1px solid #E6E6E6;padding: 10px;}
#homePage .surveyDepict table tbody tr td:first-child{width: 64px;color: #666666;padding-right: 0px;}

.surveyDepict{margin-top: 20px;}
.surveyDepict .depictTitle{text-align: center;font-size: 17px;font-weight: 900;color: #333333!important;}
#homePage .surveySubmitFooter{position: static;width: 9rem;  margin: auto;  border-radius: 20px;  }


/*问卷首页信息简介 end*/

#page{min-height: 100%;  height: auto!important;  height: 100%;  position: relative;  padding-bottom:20px;}
.content{margin: auto;position: relative;max-width: 800px;}
.survey-img{width: 100%;height: auto;position: fixed;max-width: 820px;}
.survey-img img{display: block;width: 100%;  height: 4rem;}
/*.survey-content{height: calc(100% - 144px);} 修改为calc(100% - 200px)   20171031 by 文霞*/
.survey-content{padding: 0 10px 40px 10px;  padding-top: 0px;font-size: 14px;color: #333333;position: fixed;top: 4rem;height: calc(100% - 200px);overflow: auto;width: calc(100% - 20px);max-width: 800px;}


.surveyFooterLink{text-align: center;   width: 9rem;max-width: 820px;margin: auto;background-color:#00d2ae;color: #ffffff; font-size: 18px;line-height: 40px;border-radius: 20px;}
.surveyFooterLink a{color: #ffffff;}
/*每一个问题的主体，包括外边距,提示边框颜色，问题标题，答案选项*/

/*.problemList{}*/

.problem{padding-top: 20px;clear: both;}
/*在点击提交按钮的时候验证如果没有作答则把 problemCenter 换成 problemUnanswered*/
/*默认样式*/
.problemCenter{border: 2px solid #ffffff; padding: 5px;min-width: 9rem;}
.problemCenter .problemCenterText{color: #ffffff;font-size: 11px;}
/*未作答 start*/
.problemUnanswered{border: 2px solid rgb(253, 203, 129); padding: 5px;}
.problemUnanswered .problemCenterText{color: red;font-size: 11px;margin-left: 24px;margin-top: 5px;}
/*未作答 end*/

.problemTitle{ font-size: 15px;  color: #444444;  font-weight: bold;  height: auto;  line-height: 20px;}
.problemTitle .problemNum{float: left;width: 24px;}
.problemTitle .problemWord{float: left;width: calc(100% - 24px);}
.problemTitle .problemImg{float: left;max-width: calc(100% - 24px);margin-top: 5px;}

.problemAnswer{clear: both;}

.problemAnswer input[type='text']{width: 100%;clear: both;height: 21px;padding: 8px 11px;border: 1px solid #dddddd;border-radius: 3px;box-shadow: none;}
.problemAnswer input:focus{border-color:#00B7FF; }
.problemAnswer textarea{width:100%;clear: both;height: 100px;padding: 8px 11px;border: 1px solid #ddd;border-radius: 3px;box-shadow: none;}
.problemAnswer textarea:focus{border-color:#00B7FF; }

.problemAnswerList{margin-left: 24px;}
.problemAnswerList li{cursor: pointer;clear: both;  padding-top: 12px;  color: #444444;font-size: 15px;overflow: hidden;}
.problemAnswerList li .problemAnswerInfo {width:calc(100% - 24px);float: left;}
.problemAnswerList li .problemAnswerInfo img{width:auto;height:2rem;margin-top: 5px;display: block;}


/*问卷提交按钮*/
.surveySubmitFooterBox{margin: auto;max-width: 800px}
.surveySubmitFooter{text-align: center;  position: fixed;  bottom: 0px;  width: 100%;max-width: 820px;background-color:#00d0b0;/*background-color:#009d8c;*/color: #ffffff; font-size: 18px;line-height: 40px;}
.surveySubmitFooter a,.surveySubmitFooter .btnSubmit{display: none;color: #ffffff;text-decoration:none;float: right; padding: 0 20px;}

.surveySubmitFooter .problemProgressBar{width: 65%;height: 4px;background-color: #fff930;position: relative;}
.surveySubmitFooter .upperProblem{float: left;padding: 0 20px;}
.surveySubmitFooter .alightProblem{float: right;padding:0 20px;}


/*选择，与未选择 start*/
.radios {  float: left;  width: 16px;height: 16px;  background: url(../images/radioNot.png) no-repeat;  margin-top: 3px;  margin-right: 8px;}
.radio-pitch .radios {  background: url(../images/radio.png) no-repeat;}

.checkbox {  float: left;  width: 16px;height: 16px; background: url(../images/checkboxNot.png) no-repeat;  margin-top: 3px;  margin-right: 8px;   }
.checkbox-pitch .checkbox {  background: url(../images/checkbox.png) no-repeat;  }
/*选择，与未选择 end*/

/*问卷内容居中  by 文霞 20171026*/
.content-center{display: flex;justify-content: center; align-items: Center;}
/*新调查问卷end*/


/*问卷结果分析页面 start*/
#result .section1{background-color: #FFFFFF;height: auto;padding:0.25rem; }
.section1 .taskName{margin-top: 0.67rem;font-size: 0.64rem;color:#999999;font-family: 汉仪元隆黑;text-align: center; font-weight:300; }
.section1 .explanatory{width: 7rem;height: 0.6rem;margin: auto;background-image: url("../images/explanatory.png");background-repeat: no-repeat;background-size: 100% 100%;margin-top: 0.5rem;}
.section1 .evaluationScore{height: 5.47rem;width: 100%;margin-bottom: 2rem;margin-top: 0.3rem;}
.section1 .evaluationLevel{width: 6.33rem;height: 1.8rem;margin: auto;margin-bottom: 1.68rem;}
.section1 .evaluationLevel img{width: 100%;height: 100%;}
.section1 .dateRankingBox{padding-bottom: 1.7rem;width: 100%; overflow: auto;}
.section1 .dateRankingBox .dateBox{float: left;text-align: center;width: calc(50% - 1px); border-right: 1px solid rgba(84, 124, 229, 0.2);}
.section1 .dateRankingBox .dateBox .date{color: #ee7623;font-size: 0.64rem;font-weight: bold;}

.section1 .dateRankingBox .rankingBox{float: left;text-align: center;width: calc(50% - 1px); border-left: 1px solid rgba(84, 124, 229, 0.2);}
.section1 .dateRankingBox .rankingBox .ranking{font-size: 0.64rem;color: #666666;}
.section1 .dateRankingBox .rankingBox .ranking span{color: #ee7623; }
.section1 .dateRankingBox .dataText,.section1 .dateRankingBox .rankingText{font-size: 0.36rem;color: #999999;}

#result .section2{background-color: #FFFFFF;height: auto;padding:0.25rem;  }
.section2 .graphical{height: 4rem;background-color: #2bcaae;}
.section2 .remarkTitle{width: 100%;font-size: 0.36rem;color: #666;padding-top: 0.4rem;}
.section2 .remarkTitle .titleIcon{display:inline-block;width: 0.12rem;height: 0.4rem;background-color:#666666;position: relative;top: 0.07rem;margin-right: 0.1rem; border-top-right-radius:3px;border-bottom-right-radius:3px;}


.section3 {background-color: #FFFFFF;}
.dataAnalysis{background-color: #FFFFFF;padding: 0.25rem;font-family: "思源黑体"}
.dataAnalysis .subtitle{color: #1aad19;font-size: 0.373rem;margin-top: 0.2rem}
.dataAnalysis .graphical{display: block;height: 5.5rem;width: auto;padding: 0.25rem;margin-top:0.2rem;background-color: #2bcaae;}

.dataProgressBarBox{padding: 0.25rem;background-color: #FFFFFF;overflow: auto;}
.progressBarBox{border-radius: 5px;background-color:#f6f6f8; margin-top: 0.4rem;overflow: auto;font-size: 0.37rem;    padding-bottom: 0.5rem;
    padding-top: 0.25rem;}
.progressBarBox .progressBox {padding: 0.25rem  0.25rem 0  0.25rem;}
.progressBarBox .progressBarLeft {display: inline-block;width:1.5rem;text-align: right;font-size: 0.4rem;}

.progressBarBox .progressBarRight {display: inline-block;width: 80%;text-align: left}
.progressBarBox .barTitle{color:#7f7f7f;font-size:0.36rem;margin-left: 0.9rem;  }
.progressBarBox .barBox .bar1{width: 80%;max-width:80%!important;height:0.24rem;background-color:#492f9e; display: inline-block;margin-right: 0.2rem; }
.progressBarBox .barBox .bar2{width: 43%;max-width:80%!important;height:0.24rem;background-color:#48c8bb; display: inline-block;margin-right: 0.2rem; }
.progressBarBox .barBox .bar3{width: 80%;max-width:80%!important;height:0.24rem;background-color:#1aad19; display: inline-block;margin-right: 0.2rem; }
.progressBarBox .barBox .bar4{width: 70%;max-width:80%!important;height:0.24rem;background-color:#8dcd35; display: inline-block;margin-right: 0.2rem; }
.progressBarBox .barBox .bar5{width: 64%;max-width:80%!important;height:0.24rem;background-color:#ee7623; display: inline-block;margin-right: 0.2rem; }


/*综合性评语start*/
.section4{background-color: #FFFFFF;}
.remarkBox{padding: 0.25rem;}
.remark1,.remark2,.remark3,.remark4{height: auto;overflow: hidden;position: relative;}
.remark1 .remarkList,.remark2 .remarkList,.remark3 .remarkList,.remark4 .remarkList{position: relative;}

/*综合性评语Icon*/
.commentTitle {width: 9rem;font-size:0.48rem; color: #666666;padding-left: 0.8rem;}
.analysisIcon {display: inline-block;  width: 0.4rem;  height: 0.4rem;  margin-left: 0.15rem;  margin-right: 0.15rem;  position: absolute;  top: 0.15rem;}
.commentTitle  span{display: inline-block;border-radius: 10px;color: #FFFFFF;font-size: 0.3rem; padding: 0px 10px;position: relative;bottom: 0.1rem;}
.remarkContent{color: #666666;font-size:0.36rem; padding-left: 0.8rem;padding-bottom: 0.9rem;margin-top: 0.3rem;}




.remark1{border-left: 6px solid #492f9e;margin-top: 0.4rem;}
.remark1 .analysisIcon {background-image: url("../images/analysisIcon1.png");background-size: 100% 100%; }
.remark1 .commentTitle span{background-color: #492f9e;}

.remark2{border-left: 6px solid #48c8bb;padding-top: 0.4rem;}
.remark2 .analysisIcon{background-image: url("../images/analysisIcon2.png");background-size: 100% 100%; }
.remark2 .commentTitle span{background-color: #48c8bb;}

.remark3{border-left: 6px solid #ee7623;padding-top: 0.4rem;}
.remark3 .analysisIcon {background-image: url("../images/analysisIcon3.png");background-size: 100% 100%; }
.remark3 .commentTitle span{background-color: #ee7623;}

.remark4{border-left: 6px solid #1aad19;padding-top: 0.4rem;}
.remark4 .analysisIcon {background-image: url("../images/analysisIcon4.png");background-size: 100% 100%; }
.remark4 .commentTitle span{background-color: #1aad19;}

/*综合性评语end*/

.section5{background: #FFFFFF;}
.suggestBox{padding: 0.25rem;}
.suggestBox .content{width: 90%;height: auto;background-color: #735fb4;padding: 5%;border-radius: 15px;margin-top: 0.4rem;color: #FFFFFF;font-size: 0.36rem;padding-bottom: 0.5rem;}
.suggestBox .content .icon{display: block;width:0.74rem;height: 0.53rem;background-image: url("../images/analysisIcon5.png");background-repeat: no-repeat;background-size: 100% 100%; }
.suggestBox .content .title{color:#FFFFFF;font-size:  0.48rem;margin-top: 0.5rem;padding-bottom: 0.37rem;}
.suggestBox .content .words{padding-bottom: 0.5rem;}
.suggestBox .content .partingLine{background-color: rgba(0, 0, 0, 0.05);}
.suggestBox .signature i{display: inline-block;  width: 0.86rem; height: 2px;background-color: #FFFFFF;position: relative;bottom: 0.1rem;}
.suggestBox .signature{text-align: right;padding-right: 10px;}



.words{text-indent: 2em;}
.title {color: #333333;font-size: 0.56rem; line-height: 0.6rem;}
.title .icon1{display: inline-block;width: 6px;height: 0.6rem;background-color:#1aad19;position: relative;top: 0.1rem;margin-right: 0.1rem; }
.title .icon2{display: inline-block;width: 6px;height: 0.6rem;background-color:#f1914f;position: relative;top: 0.1rem;margin-right: 0.1rem; }
.title .icon3{display: inline-block;width: 6px;height: 0.6rem;background-color:#492f9e;position: relative;top: 0.1rem;margin-right: 0.1rem; }
.partingLine{background-color: #f6f6f6;width: 100%;height: 2px;margin-top: 0.2rem;}




/*问卷结果分析页面 end*/
