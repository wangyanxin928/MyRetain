router.get("/deleteQuestionOption", function (req, res, next) {
    baseQuestionService.deleteQuestionOption(req, res, next);
});

//问卷题目所有选项
router.get("/selectQuestionOptionList", function (req, res, next) {
    baseQuestionService.selectQuestionOptionList(req, res, next);
});


module.exports = router;

